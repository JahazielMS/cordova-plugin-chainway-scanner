<?xml version='1.0' encoding='utf-8'?>
<plugin id="cordova_plugin_chainway_scanner_id" version="1.0.0"
  xmlns="http://apache.org/cordova/ns/plugins/1.0"
  xmlns:android="http://schemas.android.com/apk/res/android">
  <name>ChainwayScan</name>
  <js-module name="ChainwayScan" src="www/cordova-plugin-chainway-scanner.js">
    <clobbers target="cordova.plugins.ChainwayScan" />
  </js-module>
  <platform name="android">
    <config-file parent="/*" target="res/xml/config.xml">
      <feature name="ChainwayScannerPlugin">
        <param name="android-package" value="com.plugin.scan.ChainwayScannerPlugin" />
      </feature>
    </config-file>
    <framework src="src/android/build.gradle" custom="true" type="gradleReference" />
    <resource-file src="libs/device_api.aar" target="libs/device_api.aar" />
    <config-file parent="/*" target="AndroidManifest.xml"></config-file>
    <source-file src="src/android/ChainwayScannerPlugin.java" target-dir="src/cordova_plugin_chainway_scanner_id/cordova-plugin-chainway-scanner" />
    <source-file src="src/android/Barcode2D.java" target-dir="src/cordova_plugin_chainway_scanner_id/cordova-plugin-chainway-scanner" />
    <source-file src="src/android/IBarcodeResult.java" target-dir="src/cordova_plugin_chainway_scanner_id/cordova-plugin-chainway-scanner" />
  </platform>
</plugin>